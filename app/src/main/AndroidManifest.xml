<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mt.dingcan">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:name=".myapp.Myapp"
        android:allowBackup="true"
        android:icon="@mipmap/app_desk"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".LoginActivity" />
        <activity android:name=".RegistActivity" />
        <activity
            android:name=".ContentActivty"
            android:windowSoftInputMode="stateHidden" />
        <activity android:name=".OrderActivity" />
        <activity android:name=".PaySucceedActivity"/>
        <activity android:name=".ShipmentActivity"/>
    </application>

</manifest>